{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "semi": ["error", "never"],
    "no-extra-semi": "error",
    "no-trailing-spaces": "off",
    "no-console": "warn",
    "quotes": ["error", "single"],
    "react/prop-types": "off",
    "no-multi-spaces": "error"
  }
}
